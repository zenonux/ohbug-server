{
  "private": true,
  "name": "ohbug-server",
  "description": "Ohbug Server",
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "homepage": "https://github.com/ohbug-org/ohbug-server",
  "bugs": {
    "url": "https://github.com/ohbug-org/ohbug-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohbug-org/ohbug-server"
  },
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run lint && npm run build:dashboard && npm run build:manager && npm run build:notifier && npm run build:transfer",
    "build:dashboard": "cross-env NODE_ENV=production nest build dashboard",
    "build:manager": "cross-env NODE_ENV=production nest build manager",
    "build:notifier": "cross-env NODE_ENV=production nest build notifier",
    "build:transfer": "cross-env NODE_ENV=production nest build transfer",
    "start": "npm run docker:dev & npm run start:all",
    "start:all": "concurrently \"npm run start:dashboard\" \"npm run start:manager\" \"npm run start:notifier\" \"npm run start:transfer\" --kill-others",
    "start:dashboard": "cross-env NODE_ENV=development nest start dashboard --watch",
    "start:manager": "cross-env NODE_ENV=development nest start manager --watch",
    "start:notifier": "cross-env NODE_ENV=development nest start notifier --watch",
    "start:transfer": "cross-env NODE_ENV=development nest start transfer --watch",
    "debug": "npm run docker:dev & npm run debug:all",
    "debug:all": "concurrently \"npm run debug:dashboard\" \"npm run debug:manager\" \"npm run debug:notifier\" \"npm run debug:transfer\" --kill-others",
    "debug:dashboard": "cross-env NODE_ENV=development nest start dashboard --debug --watch",
    "debug:manager": "cross-env NODE_ENV=development nest start manager --debug --watch",
    "debug:notifier": "cross-env NODE_ENV=development nest start notifier --debug --watch",
    "debug:transfer": "cross-env NODE_ENV=development nest start transfer --debug --watch",
    "docker:dev": "docker-compose -f ./docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose --env-file .env.production -f ./docker-compose.prod.yml up --build -d",
    "test:ui": "npx majestic",
    "test": "jest",
    "lint": "eslint **/*.ts --fix",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.1",
    "@nestjs/bull": "^0.4.0",
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.5",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.1",
    "@ohbug/browser": "^1.0.17",
    "@ohbug/core": "^1.1.5",
    "@ohbug/miniapp": "^1.0.1",
    "@ohbug/react": "^1.0.13",
    "@ohbug/types": "^1.1.4",
    "@ohbug/vue": "^1.0.12",
    "bull": "^3.29.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "markdown-it": "^12.1.0",
    "nanoid": "^3.1.23",
    "nest-winston": "^1.6.0",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "ramda": "^0.27.0",
    "raw-body": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "source-map-trace": "^0.1.3",
    "typeorm": "^0.2.36",
    "web-push": "^3.4.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@chenyueban/lint": "^1.1.0",
    "@chenyueban/tsconfig": "^0.2.0",
    "@nestjs/cli": "^8.1.0",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.5",
    "@types/bull": "^3.15.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/markdown-it": "^12.2.1",
    "@types/node": "^16.7.13",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.4",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.20",
    "@types/supertest": "^2.0.11",
    "@types/web-push": "^3.3.0",
    "axios": "^0.21.4",
    "commitizen": "^4.2.4",
